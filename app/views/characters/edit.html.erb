<center class="label"><h1>Edit Character</h1>
<%= form_for :character, url: character_path(@character), method: :patch do |f| %>
  <p>
        <h4 class="label">
        <%= f.label :name %>
        </h4>
        <%= f.text_field :name %>
    </p>
    
    <p>
        <h4 class="label">
        <%= f.label :gender %>
        </h4>
        <%= f.text_field :gender %>
    </p>
    
    <p>
        <h4 class="label">
        <%= f.label :race %>
        </h4>
        <%= f.text_field :race %>
    </p>
    
    <p>
        <h4 class="label">
        <%= f.label :class %>
        </h4>
        <%= f.text_field :charclass %>
    </p>
    
    <p>
        <h4 class="label">
        <%= f.label :story %>
        </h4>
        <%= f.text_area :story %>
    </p>
    
    <p>
        <%= f.submit %>
    </p>
<% end %>
<%= link_to 'Back', characters_path %></center>
